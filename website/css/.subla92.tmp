			width:100%;
			height:42px;
			position:relative;
		}
		.home6_1_1>span{
			font-size:14px;
		}
		.home6_2{
			width:287px;
			height:435px;
			background:lime;
			float:right;
			border-radius:10px;
		}
		.square{
			top:1px;
			left:40px;
			width:160px;
			line-height:40px;
			background:#2d7cc8;
			position:relative;
			border-radius:30px;
			border-top:1px solid blue;
		}
		.square>p{
			font-size:20px;
			text-align:center;
			color:#fff;
		}
		.square1{
			/*transparent*/
			border-top:32px solid transparent;
			border-bottom:32px solid transparent;
			border-left:32px solid transparent;
			border-right:32px solid #fff;
			width:0px;
			transform:rotate(45deg);
			position:absolute;
			bottom:10px;
			left:-32px;
			}
		.square2{
			/*transparent    透明*/
			border-top:32px solid transparent;
			border-bottom:32px solid #fff;
			border-left:32px solid transparent;
			border-right:32px solid transparent;
			width:0px;
			transform:rotate(135deg);
			position:absolute;
			top:13px;
			right:-32px;
			}	
			.home6_1_2{
				width:460px;
				height:42px;
				background:red;
				position:absolute;
				top:0;right:0;
			}
		
			



			
		

			
			

			
			

		

